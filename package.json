{
  "version": "1.1.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kwaimind/tinyplural.git"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx test && tsdx build",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:coverage": "tsdx test --coverage",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "tinyplural",
  "author": "Daniel Reed",
  "module": "dist/tinyplural.esm.js",
  "size-limit": [
    {
      "path": "dist/tinyplural.cjs.production.min.js",
      "limit": "5 KB"
    },
    {
      "path": "dist/tinyplural.esm.js",
      "limit": "5 KB"
    }
  ],
  "dependencies": {},
  "resolutions": {
    "tsdx/**/node-notifier": "10.0.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.1",
    "global": "^4.4.0",
    "size-limit": "^5.0.1",
    "tsdx": "^0.14.1"
  }
}
